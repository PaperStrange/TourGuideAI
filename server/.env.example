# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGIN=http://localhost:3000

# API Key Management
ENCRYPTION_KEY=your_encryption_key_here
KEY_SALT=your_key_salt_here
KEY_ROTATION_INTERVAL=30 # days

# OpenAI API Configuration
OPENAI_KEY_ID=openai_key_1
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps API Configuration
GOOGLE_MAPS_KEY_ID=google_maps_key_1
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# JWT Authentication
JWT_SECRET=your-jwt-secret-key-here
JWT_EXPIRY=24h

# Beta Program
DEFAULT_ADMIN_EMAIL=admin@example.com
DEFAULT_ADMIN_PASSWORD=secure-password-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=30
OPENAI_RATE_LIMIT_MAX=20
MAPS_RATE_LIMIT_MAX=50

# Logging
LOG_LEVEL=info
LOG_FILE=logs/server.log

# Cache Configuration
CACHE_TTL=3600 # 1 hour
MAX_CACHE_SIZE=50 # MB

# API Configuration
OPENAI_MODEL=gpt-4o

# Security
CORS_ORIGIN=http://localhost:8000 